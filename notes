take_string(char *ptr1, char *ptr2, char **tok, char **str)
	*ptr2 = '\0';
	*tok = ft_strjoin(*tok, *ptr1);
	*str = ptr2 + 1;

process()
	while (1)
		find first quote
		find first space
		if the quote is after the space
			break
		if the quote is after the current position
			take the string from the currpos to the first quot
		if there isnt the matching quote
			return missing
		take the string from the first quote to the second
		string = second quote + 1
	take the string from currpos to space
	str = space + 1
